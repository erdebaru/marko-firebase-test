{
  "include": ["src/**/*", "vite.config.ts", ".marko-run/*"],
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"]
    },
  }
}